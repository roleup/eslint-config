{
  "name": "@roleup/eslint-config",
  "description": "ESLint config for RoleUp",
  "version": "2.0.2",
  "author": "Eric Hacke",
  "bugs": {
    "url": "https://github.com/roleup/eslint-config/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@xtrctio/eslint-plugin-disallow-date": "^1.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-lodash": "^6.0.0",
    "eslint-plugin-mocha": "^6.2.0",
    "eslint-plugin-no-secrets": "^0.3.4",
    "eslint-plugin-optimize-regex": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-simple-import-sort": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.4.0",
    "eslint-plugin-unicorn": "^12.1.0",
    "eslint-plugin-xss": "^0.1.9"
  },
  "devDependencies": {
    "@roleup/commitlint-config": "^1.0.1",
    "commitizen": "^4.0.3",
    "commitlint": "^8.2.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.5.1",
    "fixpack": "^2.3.1",
    "husky": "^3.0.9"
  },
  "files": [
    "*.js",
    "!scratch.*"
  ],
  "homepage": "https://github.com/roleup/eslint-config#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "config",
    "eslint",
    "lint"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "@roleup/prettier-config": "^1.0.2",
    "eslint": "^5.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roleup/eslint-config.git"
  },
  "scripts": {
    "commit": "git-cz",
    "prepare": "fixpack",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
