{
  "name": "@xtrctio/eslint-config",
  "description": "ESLint config for xtrct.io",
  "version": "4.0.0",
  "author": "Eric Hacke",
  "bugs": {
    "url": "https://github.com/xtrctio/eslint-config/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@xtrctio/eslint-plugin-disallow-date": "^1.0.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-mocha": "^5.2.1",
    "eslint-plugin-prettier": "^3.1.0"
  },
  "devDependencies": {
    "@xtrctio/commitlint-config": "^1.0.4",
    "commitizen": "^3.1.1",
    "commitlint": "^7.5.2",
    "cz-conventional-changelog": "^2.1.0",
    "fixpack": "^2.3.1",
    "husky": "^2.2.0"
  },
  "files": [
    "*.js",
    "!scratch.*"
  ],
  "homepage": "https://github.com/xtrctio/eslint-config#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "config",
    "eslint",
    "lint"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "@xtrctio/prettier-config": "^1.0.2",
    "eslint": "^5.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xtrctio/eslint-config.git"
  },
  "scripts": {
    "commit": "git-cz",
    "prepare": "fixpack",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
